            "entryFile": "libs/platform/react/todo/feature-list/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/platform/react/todo/feature-list/README.md",
